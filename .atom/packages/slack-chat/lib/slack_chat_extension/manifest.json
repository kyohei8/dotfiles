{
  "manifest_version": 2,

  "name": "Slack Chat",
  "description": "Send notifications to Slack Chat for Atom",
  "version": "1.1",

  "content_scripts": [{
    "matches": ["https://*.slack.com/messages/*/*"],
    "js": ["injector.js"]
  }],
  
  "web_accessible_resources": [
    "slack-scraper.js", 
    "jquery.js", 
    "underscore.js"
  ]
}